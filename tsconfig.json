{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 22",
  "type": "module",
  "_version": "22.0.0",
  "compilerOptions": {
    "lib": [
      "es2024",
      "ESNext.Array",
      "ESNext.Collection",
      "ESNext.Iterator"
    ],
    "module": "nodenext",
    "target": "es2022",
    "allowImportingTsExtensions": true,
    // TypeScript Utilizado  para apenas checagem de tipos e n√£o para bundling
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node16"
  }
}
//